{
  "build": {
    "env": {
      "react_app_api_key": "@react_app_api_key"
    }
  },
  "version": 2,
  "public": false,
  "builds": [
    { "src": "/client/package.json", "use": "@now/static-build" },
    { "src": "/index.js", "use": "@now/node-server" }
  ],
  "routes": [
    { "src": "/api/.*", "dest": "/index.js" },
    { "src": "^/static/(.*)", "dest": "/static/$1" },
    { "src": ".*", "dest": "/index.html" }
  ]
}
